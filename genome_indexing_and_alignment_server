#this file contains the code to index the yellow croaker genome and align the fastq reads if being performed on a server
hisat2-bulid GCF_000972845.1_L_crocea_1.0_genomic.fna/data yellowcroaker -p 4
hisat2 -p 4 --dta -x yellowcroaker/yellowcroaker -U trimmed_LB2A_SRR1964642.fastq -S trim_LB2A_SRR1964642.sam
hisat2 -p 4 --dta -x yellowcroaker/yellowcroaker -U trimmed_LB2A_SRR1964643.fastq -S trim_LB2A_SRR1964643.sam
hisat2 -p 4 --dta -x yellowcroaker/yellowcroaker -U trimmed_LC2A_SRR1964644.fastq -S trim_LC2A_SRR1964644.sam
hisat2 -p 4 --dta -x yellowcroaker/yellowcroaker -U trimmed_LC2A_SRR1964645.fastq -S trim_LC2A_SRR1964645.sam
